{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/post/技术/(.*)",
      "destination": "/post/tech/:slug",
      "permanent": true
    },
    {
      "source": "/post/读书/(.*)",
      "destination": "/post/readings/:slug",
      "permanent": true
    },
    {
      "source": "/post/随笔/(.*)",
      "destination": "/post/jottings/:slug",
      "permanent": true
    }
  ]
}

