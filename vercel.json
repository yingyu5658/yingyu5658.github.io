{
  "redirects": [
    {
      "source": "/post/读书",
      "destination": "/post/readings/",
      "permanent": true
    },
    {
      "source": "/post/%E8%AF%BB%E4%B9%A6",
      "destination": "/post/readings/",
      "permanent": true
    },
    {
      "source": "/post/%E8%AF%BB%E4%B9%A6/(.*)",
      "destination": "/post/readings/$1",
      "permanent": true
    },

    {
      "source": "/post/readings/:path*",
      "destination": "/posts/readings/:path*",
      "permanent": true
    },
    {
      "source": "/post/tech/:path*",
      "destination": "/posts/tech/:path*",
      "permanent": true
    },
    {
      "source": "/post/jottings/:path*",
      "destination": "/posts/jottings/:path*",
      "permanent": true
    },

    {
      "source": "/post",
      "destination": "/posts",
      "permanent": true
    },
    {
      "source": "/post/",
      "destination": "/posts/",
      "permanent": true
    }
  ]
}
